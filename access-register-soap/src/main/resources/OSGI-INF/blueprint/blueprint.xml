     <bean id="reportRegistrationRoutes" class="org.box.fuse.RegistrationRoutes"/>
     
    <camelContext id="_context1" xmlns="http://camel.apache.org/schema/blueprint">
        <route id="_route1">
            <!-- <from id="_from1" uri="file:src/data?fileName=register.xml&amp;noop=true"/>
            <to id="_to1" uri="cxf:RegistrationEndpoint"/>
            <log id="_log1" message="Success ${body}"/> -->
            <routeBuilder id="_routeBuilder1" ref="reportRegistrationRoutes"/>
        </route>
    </camelContext>
</blueprint>
